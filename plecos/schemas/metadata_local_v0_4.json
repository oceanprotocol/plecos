{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "main"
  ],
  "properties": {
    "main": {
      "$id": "#/properties/main",
      "type": "object",
      "title": "The Main Schema",
      "default": null,
      "additionalProperties": false,
      "required": [
        "name",
        "dateCreated",
        "author",
        "license",
        "price",
        "files",
        "type"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/main/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "10 Monkey Species Small"
          ],
          "pattern": "^(.*)$"
        },
        "dateCreated": {
          "$id": "#/properties/main/properties/dateCreated",
          "type": "string",
          "title": "The Datecreated Schema",
          "default": "",
          "examples": [
            "2012-02-01T10:55:11Z"
          ],
          "pattern": "^(.*)$"
        },
        "author": {
          "$id": "#/properties/main/properties/author",
          "type": "string",
          "title": "The Author Schema",
          "default": "",
          "examples": [
            "Mario"
          ],
          "pattern": "^(.*)$"
        },
        "license": {
          "$id": "#/properties/main/properties/license",
          "type": "string",
          "title": "The License Schema",
          "default": "",
          "examples": [
            "CC0: Public Domain"
          ],
          "pattern": "^(.*)$"
        },
        "price": {
          "$id": "#/properties/main/properties/price",
          "type": "string",
          "title": "The Price Schema",
          "default": "0",
          "examples": [
            "10"
          ],
          "pattern": "^[0-9]+$"
        },
        "files": {
          "$id": "#/properties/main/properties/files",
          "type": "array",
          "title": "The Files Schema",
          "items": {
            "$id": "#/properties/main/properties/files/items",
            "type": "object",
            "title": "The Items Schema",
            "additionalProperties": false,
            "required": [
              "index",
              "url",
              "contentType"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/main/properties/files/items/properties/name",
                "type": "string",
                "title": "The file Name Schema",
                "default": "",
                "examples": [
                  "monkey_species.csv"
                ],
                "pattern": "^(.*)$"
              },
              "contentType": {
                "$id": "#/properties/main/properties/files/items/properties/contentType",
                "type": "string",
                "title": "The Contenttype Schema",
                "default": "",
                "examples": [
                  "application/zip"
                ],
                "pattern": "^(.*)$"
              },
              "encoding": {
                "$id": "#/properties/main/properties/files/items/properties/encoding",
                "type": "string",
                "title": "The Encoding Schema",
                "default": "",
                "examples": [
                  "UTF-8"
                ],
                "pattern": "^(.*)$"
              },
              "compression": {
                "$id": "#/properties/main/properties/files/items/properties/compression",
                "type": "string",
                "title": "The Compression Schema",
                "default": "",
                "examples": [
                  "zip"
                ],
                "pattern": "^(.*)$"
              },
              "checksum": {
                "$id": "#/properties/main/properties/files/items/properties/checksum",
                "type": "string",
                "title": "The Checksum Schema",
                "default": "",
                "examples": [
                  "2bf9d229d110d1976cdf85e9f3256c7f"
                ],
                "pattern": "^(.*)$"
              },
              "checksumType": {
                "$id": "#/properties/main/properties/files/items/properties/checksumType",
                "type": "string",
                "title": "The Checksumtype Schema",
                "default": "",
                "examples": [
                  "MD5"
                ],
                "pattern": "^(.*)$"
              },
              "contentLength": {
                "$id": "#/properties/main/properties/files/items/properties/contentLength",
                "type": "string",
                "title": "The Contentlength Schema",
                "default": "0",
                "examples": [
                  "12057507"
                ],
                "pattern": "^[0-9]+$"
              },
              "index": {
                "$id": "#/properties/main/properties/files/items/properties/index",
                "type": "integer",
                "title": "The index Schema",
                "default": 0,
                "examples": [
                  0
                ]
              },
              "resourceId": {
                "$id": "#/properties/main/properties/files/items/properties/resourceId",
                "type": "string",
                "title": "The resourceId Schema",
                "default": 0,
                "examples": [
                  "bucket:47550"
                ]
              },
              "attributes": {
                "$id": "#/properties/main/properties/files/items/properties/attributes",
                "type": "array",
                "title": "The attributes Schema",
                "default": []
              },
              "encrypted": {
                "$id": "#/properties/main/properties/files/items/properties/encrypted",
                "type": "boolean",
                "title": "The encrypted Schema",
                "default": false,
                "examples": [
                  true
                ]
              },
              "encryptionMode": {
                "$id": "#/properties/main/properties/files/items/properties/encryptionMode",
                "type": "string",
                "title": "The encryptionMode Schema",
                "default": "",
                "examples": [
                  "CBC"
                ]
              },
              "url": {
                "$id": "#/properties/main/properties/files/items/properties/url",
                "type": "string",
                "title": "The Url Schema",
                "default": "",
                "examples": [
                  "https://s3.amazonaws.com/assets/training.zip"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "type": {
          "$id": "#/properties/main/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "",
          "examples": [
            "dataset"
          ],
          "pattern": "^(.*)$"
        },
        "algorithm": {
          "$id": "#/properties/main/properties/algorithm",
          "type": "object",
          "title": "The algorithm Schema",
          "required": [
            "language",
            "entrypoint",
            "requirements"
          ],
          "properties": {
            "language": {
              "$id": "#/properties/main/properties/algorithm/properties/language",
              "type": "string",
              "title": "The algorithm language Schema"
            },
            "format": {
              "$id": "#/properties/main/properties/algorithm/properties/format",
              "type": "string",
              "title": "The algorithm format Schema"
            },
            "version": {
              "$id": "#/properties/main/properties/algorithm/properties/version",
              "type": "string",
              "title": "The algorithm version Schema"
            },
            "entrypoint": {
              "$id": "#/properties/main/properties/algorithm/properties/entrypoint",
              "type": "string",
              "title": "The algorithm entrypoint Schema"
            },
            "requirements": {
              "$id": "#/properties/main/properties/algorithm/properties/requirements",
              "type": "array",
              "title": "The algorithm requirements Schema"
            }
          }
        },
        "workflow": {
          "$id": "#/properties/main/properties/workflow",
          "type": "object",
          "title": "The workflow Schema",
          "properties": {
            "stages": {
              "$id": "#/properties/main/properties/workflow/properties/stages",
              "type": "array",
              "title": "The workflow stages Schema",
              "items": {
                "$id": "#/properties/main/properties/workflow/properties/stages/items",
                "type": "object",
                "title": "The Items Schema",
                "properties": {
                  "index": {
                    "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/index",
                    "type": "integer",
                    "title": "The stage index Schema"
                  },
                  "stageType": {
                    "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/stageType",
                    "type": "string",
                    "title": "The stageType Schema"
                  },
                  "requirements": {
                    "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/requirements",
                    "type": "object",
                    "title": "The stage requirements Schema"
                  },
                  "input": {
                    "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/input",
                    "type": "array",
                    "title": "The stage input Schema",
                    "items": {
                      "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/input/items",
                      "properties": {
                        "index": {
                          "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/input/items/properties/index",
                          "type": "integer"
                        },
                        "id": {
                          "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/input/items/properties/id",
                          "type": "string"
                        }
                      }
                    }
                  },
                  "transformation": {
                    "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/transformation",
                    "type": "string",
                    "title": "The stage transformation Schema"
                  },
                  "output": {
                    "$id": "#/properties/main/properties/workflow/properties/stages/items/properties/output",
                    "type": "object",
                    "title": "The stage output Schema"
                  }
                }
              }
            }
          }
        },
        "service": {
          "$id": "#/properties/main/properties/service",
          "type": "object",
          "title": "The service Schema"
        }
      }
    },
    "additionalInformation": {
      "$id": "#/properties/additionalInformation",
      "type": "object",
      "title": "The AdditionalInformation Schema",
      "default": null,
      "properties": {
        "description": {
          "$id": "#/properties/additionalInformation/properties/description",
          "type": "string",
          "title": "The Description Schema",
          "default": "",
          "examples": [
            "EXAMPLE ONLY "
          ],
          "pattern": "^(.|\\s)*$"
        },
        "categories": {
          "$id": "#/properties/additionalInformation/properties/categories",
          "type": "array",
          "title": "The Categories Schema",
          "default": null,
          "items": {
            "$id": "#/properties/additionalInformation/properties/categories/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "image"
            ],
            "pattern": "^(.*)$"
          }
        },
        "tags": {
          "$id": "#/properties/additionalInformation/properties/tags",
          "type": "array",
          "title": "The Tags Schema",
          "default": null,
          "items": {
            "$id": "#/properties/additionalInformation/properties/tags/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "image data",
              "classification",
              "animals"
            ],
            "pattern": "^(.*)$"
          }
        },
        "copyrightHolder": {
          "$id": "#/properties/additionalInformation/properties/copyrightHolder",
          "type": "string",
          "title": "The Copyrightholder Schema",
          "default": "",
          "examples": [
            "Unknown"
          ],
          "pattern": "^(.*)$"
        },
        "workExample": {
          "$id": "#/properties/additionalInformation/properties/workExample",
          "type": "string",
          "title": "The Workexample Schema",
          "default": "",
          "examples": [
            "image path, id, label"
          ],
          "pattern": "^(.*)$"
        },
        "links": {
          "$id": "#/properties/additionalInformation/properties/links",
          "type": "array",
          "title": "The Links Schema",
          "default": null,
          "items": {
            "$id": "#/properties/additionalInformation/properties/links/items",
            "type": "object",
            "title": "The Items Schema",
            "default": null,
            "additionalProperties": true,
            "required": [
              "url"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/additionalInformation/properties/links/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "example model"
                ],
                "pattern": "^(.*)$"
              },
              "type": {
                "$id": "#/properties/additionalInformation/properties/links/items/properties/type",
                "type": "string",
                "title": "sample link",
                "default": "",
                "examples": [
                  "https://sample.com"
                ],
                "pattern": "^(.*)$"
              },
              "url": {
                "$id": "#/properties/additionalInformation/properties/links/items/properties/url",
                "type": "string",
                "title": "The Url Schema",
                "default": "",
                "examples": [
                  "https://drive.google.com/open?id=1uuz50RGiAW8YxRcWeQVgQglZpyAebgSM"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "inLanguage": {
          "$id": "#/properties/additionalInformation/properties/inLanguage",
          "type": "string",
          "title": "The Inlanguage Schema",
          "default": "",
          "examples": [
            "en"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}